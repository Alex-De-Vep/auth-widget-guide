		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/typography": "^0.5.19",
		"@types/node": "^20",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"gh-pages": "^6.3.0",
		"globals": "^16.0.0",
		"mdsvex": "^0.12.6",
		"postcss": "^8.5.6",
